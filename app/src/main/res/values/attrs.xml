<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DotProgressBar">
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
        <attr name="duration" format="integer" />
        <attr name="amount" format="integer" />
        <attr name="animationDirection" format="enum">
            <enum name="right" value="1" />
            <enum name="left" value="-1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SubmitView">
        <attr name="is_submit" format="boolean" />
        <attr name="text_submit" format="string" />
    </declare-styleable>
</resources>